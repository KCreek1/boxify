        {
            "name": "examplify (profile mode)",
            "cwd": "examplify",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile"
        },
        {
            "name": "examplify (release mode)",
            "cwd": "examplify",
            "request": "launch",
            "type": "dart",
